nbsp;&amp;nbsp;&amp;nbsp;&lt;subNetwork&amp;nbsp;name="lan3"&amp;nbsp;label="Lan&amp;nbsp;3"&amp;nbsp;mask="10.0.0.0/8"&amp;nbsp;proxy="127.0.0.1,&amp;nbsp;::1"/&gt;!!discoiqbr!!&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&lt;subNetwork&amp;nbsp;name="localhost"&amp;nbsp;label="Localhost"&amp;nbsp;mask="127.0.0.0/8"&amp;nbsp;proxy="127.0.0.1,&amp;nbsp;::1"/&gt;!!discoiqbr!!&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&lt;subNetwork&amp;nbsp;name="lan6"&amp;nbsp;&amp;nbsp;label="Lan&amp;nbsp;(IPv6)"&amp;nbsp;mask="fc00::/7"&amp;nbsp;proxy="127.0.0.1,&amp;nbsp;::1"/&gt;!!discoiqbr!!&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&lt;subNetwork&amp;nbsp;name="lan6b"&amp;nbsp;label="Lan&amp;nbsp;(IPv6)"&amp;nbsp;mask="fe80::/10"&amp;nbsp;proxy="127.0.0.1,&amp;nbsp;::1"/&gt;!!discoiqbr!!&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&lt;subNetwork&amp;nbsp;name="localhost6"&amp;nbsp;label="Localhost&amp;nbsp;(IPv6)"&amp;nbsp;mask="::1/128"&amp;nbsp;proxy="127.0.0.1,&amp;nbsp;::1"/&gt;!!discoiqbr!!&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&lt;/securityZone&gt;!!discoiqbr!!!!discoiqbr!!&amp;nbsp;&amp;nbsp;&lt;/securityZone&gt;!!discoiqbr!!&lt;/securityZone&gt; 
